{
  "model": {
    "fc_model": {
      "sizes": [
        64
      ],
      "input_norm": false,
      "activation": null,
      "dropout": 0.0
    },
    "transformer": {
      "N": 1,
      "d_ff": 64,
      "h": 1,
      "div_output" : "max",
      "positional_encoding": null,
      "dropout": 0.0
    },
    "post_model": {
      "output_activation": "Sigmoid",
      "d_output": 1
    }
  },
  "data": {
    "path": "/allrank/dummy_data",
    "validation_ds_role": "vali",
    "num_workers": 1,
    "batch_size": 64,
    "slate_length": 26
  },
  "optimizer": {
    "name": "Adam",
    "args": {
      "lr": 0.001
    }
  },
  "lr_scheduler": {
    "name": "StepLR",
    "args": {
      "step_size": 3,
      "gamma": 0.5
    }
  },
  "training": {
    "epochs": 5,
    "early_stopping_patience": 100,
    "gradient_clipping_norm": null
  },
  "val_metric": "ndcg@5",
  "metrics": [
    "ndcg@5", "ndcg@10"
  ],
  "loss": {
    "name": "lambdaLoss",
    "args": {
      "weighing_scheme": "ndcgLoss2PP_scheme",
      "k": 5000,
      "mu": 10,
      "sigma": 1.0
    }
  },
  "expected_metrics" : {
    "val": {
      "ndcg@5": 0.6
    }
  },
  "detect_anomaly" : true
}